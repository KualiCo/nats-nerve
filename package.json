{
  "name": "nats-nerve",
  "version": "1.0.3",
  "description": "High level driver for nats streaming server",
  "main": "index.js",
  "keywords": [],
  "author": "kuali",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KualiCo/nats-nerve.git"
  },
  "bugs": {
    "url": "https://github.com/kualico/nats-nerve/issues"
  },
  "scripts": {
    "test": "standard && npm run test:jest && codecov -t 55cf8294-0ad6-449b-8079-611038bde722",
    "test:jest": "NODE_ENV=test jest --runInBand"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 70,
        "branches": 70,
        "lines": 70,
        "functions": 70
      }
    },
    "forceExit": true,
    "mapCoverage": true,
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "standard": {
    "env": [
      "node",
      "jest"
    ],
    "ignore": [
      "coverage"
    ]
  },
  "files": [
    "src",
    "index.js"
  ],
  "devDependencies": {
    "bluebird": "3.5.1",
    "codecov": "^3.0.0",
    "jest": "22.0.4",
    "standard": "10.0.3"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "node-nats-streaming": "0.0.26",
    "shortid": "2.2.8"
  }
}
